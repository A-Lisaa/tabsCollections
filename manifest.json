{
    "manifest_version": 3,
    "name": "Tabs Collections",
    "version": "0.1",

    "action": {
        "default_title": "Tabs Collections"
    },

    "permissions": [
        "notifications",
        "storage",
        "tabs",
        "menus"
    ],

    "background": {
        "scripts": ["./scripts/background.js"],
        "type": "module"
    }
}